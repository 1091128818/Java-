import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		double sum=0;
		int i;
		Double M = (double) Integer.parseInt(null);
		int N = Integer.parseInt(null);
		sum = M;
		for(i=2;i<=N;i++) {
			M=M/2.0;
			if(i>1) {
				sum=sum+M;
			}
			if(i==N) {
				M=M/2.0;
			}
		}
		System.out.print(M+" ");
		System.out.print(N);
	}

}
