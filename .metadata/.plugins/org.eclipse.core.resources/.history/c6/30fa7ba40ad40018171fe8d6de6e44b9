import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner M = new Scanner(System.in);
		Scanner N = new Scanner(System.in);
		double sum=0;
		int i;
		int M = Integer.parseInt(null);
		int N = Integer.parseInt(null);
		sum = M;
		for(i=2;i<=N;i++) {
			M=(int) (M/2.0);
			if(i>1) {
				sum=sum+M;
			}
			if(i==N) {
				M=(int) (M/2.0);
			}
		}
		System.out.print(M+" ");
		System.out.print(N);
	}

}
