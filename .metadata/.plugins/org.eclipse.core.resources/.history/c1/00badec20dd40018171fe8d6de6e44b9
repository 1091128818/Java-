import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner  s = new Scanner(System.in);
		 double M = s.nextDouble();
		int N = s.nextInt();
		double sum=0;
		int i;
		sum = M;
		for(i=2;i<=N;i++) {
			M= (M/2.0);
			if(i>1) {
				sum=sum+M;
			}
			if(i==N) {
				M=(M/2.0);
			}
		}
		System.out.print(M+" ");
		System.out.print(N);
	}

}
