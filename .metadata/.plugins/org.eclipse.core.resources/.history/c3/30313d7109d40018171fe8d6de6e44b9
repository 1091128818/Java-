
public class Main {

	public static void main(String[] args) {
		double M,sum=0;
		int N,i;
		M = Integer.parseInt("M");
		N = Integer.parseInt("N");
		sum = M;
		for(i=2;i<=N;i++) {
			M=M/2.0;
			if(i>1) {
				sum=sum+M;
			}
			if(i==N) {
				M=M/2.0;
			}
		}
		System.out.print(M+" ");
		System.out.print(N);
	}

}
